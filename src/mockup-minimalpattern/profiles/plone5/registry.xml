    <value key="css">
      <element>++resource++minimalpattern/less/minimalpattern.less</element>
    </value>
  </records>

  <records prefix="plone.bundles/bundle-minimalpattern"
            interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
      <element>bundle-minimalpattern</element>
    </value>
    <value key="depends">plone</value>
    <value key="enabled">True</value>
    <value key="compile">True</value>
  </records>

</registry>
