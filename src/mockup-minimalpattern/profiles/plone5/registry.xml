      <value key="css">
        <element>++resource++minimalpattern/patterns/minimalpattern/pattern.minimalpattern.less</element>
      </value>
  </records>

  <records prefix="plone.resources/bundle-minimalpattern"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
